{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": "d7d856ed-cb91-407c-9fad-19343fea3ff6",
    "defaultCollection": "@nrwl/angular"
  },
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "beliefs-in-wreaths": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "apps/beliefs-in-wreaths",
      "sourceRoot": "apps/beliefs-in-wreaths/src",
      "prefix": "b-in-w",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/beliefs-in-wreaths",
            "index": "apps/beliefs-in-wreaths/src/index.html",
            "main": "apps/beliefs-in-wreaths/src/main.ts",
            "polyfills": "apps/beliefs-in-wreaths/src/polyfills.ts",
            "tsConfig": "apps/beliefs-in-wreaths/tsconfig.app.json",
            "assets": [
              "apps/beliefs-in-wreaths/src/favicon.ico",
              "apps/beliefs-in-wreaths/src/assets"
            ],
            "styles": ["apps/beliefs-in-wreaths/src/styles.scss"],
            "scripts": [],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/beliefs-in-wreaths/src/environments/environment.ts",
                  "with": "apps/beliefs-in-wreaths/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ]
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "beliefs-in-wreaths:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "beliefs-in-wreaths:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "beliefs-in-wreaths:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/beliefs-in-wreaths/src/test.ts",
            "polyfills": "apps/beliefs-in-wreaths/src/polyfills.ts",
            "tsConfig": "apps/beliefs-in-wreaths/tsconfig.spec.json",
            "karmaConfig": "apps/beliefs-in-wreaths/karma.conf.js",
            "assets": [
              "apps/beliefs-in-wreaths/src/favicon.ico",
              "apps/beliefs-in-wreaths/src/assets"
            ],
            "styles": ["apps/beliefs-in-wreaths/src/styles.scss"],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/beliefs-in-wreaths/tsconfig.app.json",
              "apps/beliefs-in-wreaths/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "beliefs-in-wreaths:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "beliefs-in-wreaths:serve:production"
            }
          }
        },
        "deploy": {
          "builder": "angular-cli-ghpages:deploy",
          "options": {}
        }
      }
    },
    "beliefs-in-wreaths-e2e": {
      "root": "apps/beliefs-in-wreaths-e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "apps/beliefs-in-wreaths-e2e/protractor.conf.js",
            "devServerTarget": "beliefs-in-wreaths:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "beliefs-in-wreaths:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps/beliefs-in-wreaths-e2e/tsconfig.json",
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "core": {
      "projectType": "library",
      "root": "libs/core",
      "sourceRoot": "libs/core/src",
      "prefix": "b-in-w",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/core/src/**/*.ts",
              "libs/core/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/core"],
          "options": {
            "jestConfig": "libs/core/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "shared-ui": {
      "projectType": "library",
      "root": "libs/shared-ui",
      "sourceRoot": "libs/shared-ui/src",
      "prefix": "b-in-w",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared-ui/src/**/*.ts",
              "libs/shared-ui/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared-ui"],
          "options": {
            "jestConfig": "libs/shared-ui/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "defaultProject": "beliefs-in-wreaths",
  "schematics": {
    "@nrwl/angular:application": {
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:library": {
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {}
  }
}
